<launch>

  <!-- rosbag -->
  <!-- <node name="rosbag" pkg="rosbag" type="record" args="record -O /home/nvidia/xycar_ws/src/nahye.bag /usb_cam/image_raw /scan" /> -->
  <!-- <node name="rosbag" pkg="rosbag" type="record" args="record -O /home/nvidia/xycar_ws/src/nahye.bag /usb_cam/image_raw" /> -->
  
  <!-- usb_cam -->
  <node name="usb_cam" pkg="usb_cam" type="usb_cam_node" output="screen" >
    <param name="video_device" value="/dev/videoCAM" />
    <param name="image_width" value="640" />
    <param name="image_height" value="480" />
    <param name="pixel_format" value="yuyv" />
    <param name="camera_name" value="usb_cam" />
    <param name="camera_frame_id" value="usb_cam" />
    <param name="autoexposure" value="false" />
    <param name="exposure" value="50" />
    <param name="io_method" value="mmap"/>
    <param name="camera_info_url" value="file://$(find usb_cam)/calibration/usb_cam.yaml" />
  </node>

  <!-- <node name="yolov7" pkg="yolov7" type="utils.py" output="screen" /> -->
  <node name="yolov7" pkg="yolov7" type="yolo_lidar.py" output="screen" />
  <include file="/home/nvidia/xycar_ws/src/lane_detection_offline-main/launch/drive1.launch"/>
  <!-- <include file="/home/nvidia/xycar_ws/src/sensor_fusion/launch/fusion.launch"/> -->

  <!-- lidar -->
  <include file="$(find xycar_lidar)/launch/lidar_noviewer.launch" />


</launch>
